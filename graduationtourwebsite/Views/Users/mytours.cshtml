@model IEnumerable<tour>
@{
    ViewData["title"] = "Users";
}

<h1 class="title-d">Work schedule</h1>


<table class="table table-striped mt-4">
    <thead>
<tr bgcolor="LightGreen" class='btn-bot'>

  
    <th>Client Name</th>
    <th>Client PhoneNUM</th>
    <th>From</th>
    <th>To</th>
    <th>TG Name</th>
    <th>Place</th>


</tr>

    </thead>

    <tbody>
       @foreach(var tour in Model){
            <tr>
              
               <td>@tour.custid</td>
               <td>@tour.clientphone</td>
               <td>@tour.FromDate</td>
               <td>@tour.ToDate</td>
               <td>@tour.tourguidename</td>
               <td>@tour.plces</td>
               
            </tr>

       }
        



    </tbody>
  

</table>